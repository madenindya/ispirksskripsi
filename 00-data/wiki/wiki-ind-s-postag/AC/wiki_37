<start>_VB Nama_NN Sukutukang_NN tidak_NEG terlepas_VB dari_IN sejarah_NN keberadaan_NN kampung_NN itu_PR sendiri_NN ._Z <end>_MD
<start>_VB Suku_NN merujuk_VB pada_IN marga_NN ,_Z dan_CC tukang_NN mengandung_VB arti_NN tengah_NN ._Z <end>_MD
<start>_VB Secara_IN harafiah_NN ,_Z Sukutukang_NN berarti_VB Marga_NNP Tengah_NNP ,_Z Marga_NNP yang_SC menempati_VB daerah_NN yang_SC di_IN sekelilingnya_NN sudah_MD di_IN tempati_NN oleh_IN marga-marga_NN lain_JJ ._Z <end>_MD
<start>_VB Dari_IN situlah_NN nama_NN itu_PR muncul_VB ._Z <end>_NNP
<start>_NNP Desa_NNP Pululera_NNP memiliki_VB jumlah_NN penduduknya_NN sebagian_CD besar_JJ bersuku_NN daerah_NN Flores.Ada_VB beberapa_CD marga_NN yang_SC menempati_VB desa_NN Pululera_NN ,_Z antara_IN lain_JJ :_Z Marga_NNP Tukan_NNP ,_Z Marga_NNP Seda_NNP ,_Z Marga_NNP Soge_NNP ,_Z Marga_NNP Werang_NNP ,_Z Marga_NNP Danga_NNP ,_Z Odung_NNP ,_Z Wawin_NNP ,_Z Wato_NN dll_X ._Z Sebagian_CD besar_JJ penduduknya_NN bermatapencaharian_NN petani_NN ._Z <end>_MD
<start>_VB Hasil_NN pertanian_NN utama_JJ di_IN desa_NN ini_PR ialah_VB kemiri_NN ,_Z kopi_NN dan_CC kelapa_NN ,_Z padi_NN ,_Z jagung_NN ,_Z pisang_NN ,_Z dan_CC akhir-akhir_RB ini_PR sejak_IN thn_NN 2000-an_NN mulai_VB dikembangkan_VB tanaman_NN kakao_NN ._Z <end>_NNP
<start>_NNP Ternak_NNP yang_SC cukup_RB banyak_CD dipelihara_NN adalah_VB sapi_NNP ,_Z kambing_NNP ,_Z kuda_NNP ,_Z anjing_NNP ,_Z ayam_NN dll_X ._Z <end>_MD
<start>_VB Hingga_IN saat_NN ini_PR masyarakat_NN Desa_NNP Pululera_NNP masih_MD menjalankan_VB sistem_NN gotong_NN royong_NN dlm_NN mengerjakan_VB ladang_NN mereka_PRP ._Z <end>_MD
